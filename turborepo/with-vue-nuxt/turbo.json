{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".output/**", "dist/**"]
    },
    "repo": {
      "dependsOn": ["^repo"]
    },
    "lint": {
      "dependsOn": ["repo"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
