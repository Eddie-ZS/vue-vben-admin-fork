{
	"name": "vue-vbird-admin",
	"version": "1.0.0",
	"private": true,
	"keywords": [
		"vue"
	],
	"type": "module",
	"license": "MIT",
	"author": {
		"name": "Eddie"
	},
	"scripts": {
		"dev": "turbo run dev",
		"dev:ele": "pnpm --filter @vbird/web-element run dev",
		"build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo run build",
		"build:ele": "pnpm --filter @vbird/web-element run build",
		"commit": "czg",
		"prettier": "prettier --check .",
		"prepare": "husky",
		"lint": "eslint .",
		"clean": "node ./scripts/clean.mjs",
		"postinstall": "turbo run stub",
		"reinstall": "pnpm clean --del-lock && pnpm install"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.11",
		"@types/node": "^22.10.2",
		"@vbird/commitlint-config": "workspace:*",
		"@vbird/eslint-config": "workspace:*",
		"@vbird/node-utils": "workspace:*",
		"@vbird/prettier-config": "workspace:*",
		"@vbird/stylelint-config": "workspace:*",
		"@vbird/tsconfig": "workspace:*",
		"@vbird/vite-config": "workspace:*",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitejs/plugin-vue-jsx": "^4.1.1",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"cspell": "^8.17.1",
		"husky": "9.1.7",
		"lint-staged": "^15.2.11",
		"rimraf": "^6.0.1",
		"tailwindcss": "^3.4.17",
		"turbo": "2.3.3",
		"typescript": "^5.7.2",
		"unbuild": "^3.0.1",
		"vite": "6.0.5",
		"vitest": "^2.1.8",
		"vue": "3.5.13",
		"vue-tsc": "^2.2.0"
	},
	"engines": {
		"node": ">=20",
		"pnpm": ">=9"
	},
	"packageManager": "pnpm@9.14.4",
	"pnpm": {
		"peerDependenciesRules": {
			"allowedVersions": {
				"eslint": "*"
			}
		},
		"overrides": {
			"vue": "3.5.13"
		}
	}
}
